{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "eslint-plugin-react",
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "error",
    "react/jsx-curly-brace-presence": ["error", "never"],
    "no-console": ["error", {
      "allow": ["warn", "error"]
    }]
  }
}
